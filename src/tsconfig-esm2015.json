{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es2015",
    "baseUrl": "../.packaging",
    "moduleResolution": "node",
    "outDir": "../.packaging/esm2015",
    "rootDir": "../.packaging",
  },
  "files": [
    "../.packaging/index.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "francette.js",
    "flatModuleId": "francette",
    "generateCodeForLibraries": true
  }
}
