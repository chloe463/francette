{
  "name": "francette",
  "version": "0.9.0-RC.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "changelog": "conventional-changelog --preset angular -i CHANGELOG.md -s -r 0",
    "build": "./build.sh all",
    "build:global-css": "./build.sh css",
    "build:inline": "ts-node scripts/build-css.ts && ts-node scripts/inline-resources.ts",
    "build:esm2015": "ngc -p src/tsconfig-esm2015.json",
    "build:fesm2015": "rollup -c rollup-esm2015.conf.js",
    "build:esm5": "ngc -p src/tsconfig-esm5.json",
    "build:fesm5": "rollup -c rollup-esm5.conf.js",
    "build:umd": "rollup -c rollup-umd.conf.js",
    "build:umd-min": "uglifyjs builds/bundles/francette.umd.js -c -m -o builds/bundles/francette.umd.min.js"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "2.5.6",
    "rxjs": "^6.0.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "10.0.4",
    "autoprefixer": "8.4.1",
    "codelyzer": "4.3.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.0.0",
    "postcss-cli": "^5.0.0",
    "protractor": "5.3.1",
    "rollup": "0.58.2",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "~2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chloe463/francette"
  }
}
